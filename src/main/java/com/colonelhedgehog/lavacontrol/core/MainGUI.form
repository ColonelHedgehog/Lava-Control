<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.colonelhedgehog.lavacontrol.core.MainGUI">
  <grid id="27dc6" binding="MainPanel" layout-manager="FormLayout">
    <rowspec value="center:35px:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <rowspec value="top:3dlu:noGrow"/>
    <rowspec value="center:438px:grow"/>
    <rowspec value="center:42px:noGrow"/>
    <rowspec value="top:3dlu:noGrow"/>
    <rowspec value="center:11px:noGrow"/>
    <colspec value="fill:46dlu:noGrow"/>
    <colspec value="fill:490px:grow"/>
    <colspec value="fill:111px:noGrow"/>
    <colspec value="right:103px:noGrow"/>
    <constraints>
      <xy x="24" y="20" width="897" height="600"/>
    </constraints>
    <properties>
      <minimumSize width="800" height="600"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="bfb36" class="javax.swing.JTextField" binding="jarPath">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="-1"/>
          </grid>
          <forms top="0" left="10" bottom="0" right="10" defaultalign-horz="false"/>
        </constraints>
        <properties>
          <text value="/"/>
          <toolTipText value="Press &quot;return&quot; in the field to update the jar list. "/>
        </properties>
      </component>
      <component id="2bf2d" class="javax.swing.JButton" binding="chooseJar">
        <constraints>
          <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties>
          <text value="&amp;Choose Jar..."/>
          <toolTipText value="Put the directory to your Bukkit/Spigot Jar here."/>
        </properties>
      </component>
      <scrollpane id="ca4c7" binding="pluginList">
        <constraints>
          <grid row="2" column="0" row-span="2" col-span="4" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <forms top="10" left="10" bottom="10" right="10" defaultalign-horz="false" defaultalign-vert="false"/>
        </constraints>
        <properties>
          <autoscrolls value="false"/>
          <horizontalScrollBarPolicy value="31"/>
          <opaque value="false"/>
          <toolTipText value="Uncheck a plugin and it will be renamed with a ._jar extension. Check to set the extension back to a .jar extension."/>
          <verticalScrollBarPolicy value="20"/>
        </properties>
        <clientProperties>
          <html.disable class="java.lang.Boolean" value="true"/>
        </clientProperties>
        <border type="bevel-lowered">
          <font/>
        </border>
        <children/>
      </scrollpane>
      <component id="4da08" class="javax.swing.JButton" binding="launchJar">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="4" vsize-policy="0" hsize-policy="3" anchor="1" fill="0" indent="0" use-parent-layout="false"/>
          <forms top="0" left="0" bottom="2" right="0" defaultalign-horz="false" defaultalign-vert="false"/>
        </constraints>
        <properties>
          <maximumSize width="200" height="60"/>
          <minimumSize width="200" height="60"/>
          <preferredSize width="200" height="60"/>
          <selected value="false"/>
          <text value="Start Server"/>
          <verifyInputWhenFocusTarget value="false"/>
        </properties>
      </component>
      <component id="e78db" class="javax.swing.JLabel">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms top="0" left="10" bottom="0" right="0"/>
        </constraints>
        <properties>
          <text value="Plugins:"/>
        </properties>
      </component>
      <component id="2f311" class="javax.swing.JLabel">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties>
          <enabled value="false"/>
          <text value="(Check and uncheck to enable and disable)"/>
        </properties>
      </component>
      <component id="34b3a" class="javax.swing.JButton" binding="optionsButton">
        <constraints>
          <grid row="0" column="3" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <forms top="0" left="0" bottom="0" right="5"/>
        </constraints>
        <properties>
          <text value="&amp;Options..."/>
          <toolTipText value="Put the directory to your Bukkit/Spigot Jar here."/>
        </properties>
      </component>
    </children>
  </grid>
</form>
